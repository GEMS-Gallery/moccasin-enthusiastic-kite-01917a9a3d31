type GroceryItem = 
 record {
   category: text;
   completed: bool;
   id: nat;
   isPredefined: bool;
   name: text;
 };
service : {
  addItem: (text, text, bool) -> (nat);
  getItems: () -> (vec GroceryItem) query;
  getPredefinedItems: () -> (vec text) query;
  markItemComplete: (nat) -> (bool);
  removeItem: (nat) -> (bool);
}
